[user]
    name = gerard sychay
    email = hellogerard@gmail.com
[core]
    ;autocrlf = true
    autocrlf = false
    safecrlf = true
    editor = gvim
    excludesfile = ~/.gitignore
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%Cred%h%Creset %C(bold blue)%ad%Creset | %s%C(yellow)%d%Creset %Cgreen[%an]%Creset\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
